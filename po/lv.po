msgstr "Noņemt iezīmi vai ifd"

#: exif/main.c:189
msgid "Show description of tag"
msgstr "Rādīt apraksta iezīmi"

#: exif/main.c:191
msgid "Extract thumbnail"
msgstr "Izvilkt sīktēlu"

#: exif/main.c:193
msgid "Remove thumbnail"
msgstr "Noņemt sīktēlu"

