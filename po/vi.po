#, c-format
msgid "'%s' does not contain tag '%s'."
msgstr "« %s » không chứa thẻ « %s »."

#: exif/actions.c:346
#, c-format
msgid "'%s' does not contain a thumbnail!"
msgstr "IFD « %s » không chứa ảnh mẫu."

